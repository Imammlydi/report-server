      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Add Bank Account</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
            <form action="/admin/bank" method="POST" enctype="multipart/form-data">
                <div class="form-group">
                  <label for="exampleInputEmail1">Name of Bank</label>
                  <input type="text" class="form-control" id="name" name="nameBank"  placeholder="Enter Name of Bank" required="true">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">Nomer Rekening</label>
                  <input type="text" class="form-control" id="name" name="nomerRekening"  placeholder="Enter Nomer Rekening" required="true">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">Name</label>
                  <input type="text" class="form-control" id="name" name="name"  placeholder="Name" required="true">
                </div>
                <div class="form-group">
                  <label for="exampleInputEmail1">Image</label>
                  <input type="file" class="form-control" id="name" name="image"   required="true">
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button type="submit" class="btn btn-primary">Add</button>
                </div>
              </form>
        </div>
      </div>
    </div>
  </div>